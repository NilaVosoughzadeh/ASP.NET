
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <h1>Index</h1>
        <h2>@ViewBag.data</h2>

        <ul>
            @foreach (string item in ViewBag.name)
            {
                <li>
                    @item
                </li>
            }
        </ul>
    </div>
</body>
</html>
